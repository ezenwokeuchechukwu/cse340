<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools" class="top-nav">
    <% if (loggedin) { %>
      <a title="Manage account" href="/account/">Account</a>
      <% if (accountData.account_type === "Client") { %>
        <a title="View your shopping cart" href="/cart">Cart (<%= cartCount %>)</a>
      <% } %>
      <a title="Logout" href="/account/logout">Logout</a>
    <% } else { %>
      <a title="Log in" href="/account/login">My Account</a>
    <% } %>
  </div>
  <link rel="stylesheet" href="/css/styles.css" media="screen" />
</header>
